{
  "name": "@use-tauri/monorepo",
  "version": "0.0.3",
  "private": true,
  "packageManager": "pnpm@8.6.0",
  "description": "",
  "author": "MuRong <hi@mrong.me>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/murongg",
  "homepage": "https://github.com/murongg/use-tauri#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/murongg/use-tauri.git"
  },
  "bugs": {
    "url": "https://github.com/murongg/use-tauri/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf packages/**/dist && rollup -c",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "publish": "esno scripts/build.ts && esno scripts/publish.ts",
    "release": "bumpp --all && npm run publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@types/react": "^18.2.19",
    "consola": "^3.2.3",
    "esbuild-register": "^3.4.2"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.4",
    "@antfu/utils": "^0.7.4",
    "@murongg/eslint-config": "^0.2.0",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@tauri-apps/api": "^1.4.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.16.16",
    "@vue/composition-api": "^1.7.1",
    "bumpp": "^9.1.0",
    "eslint": "^8.41.0",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.1",
    "lint-staged": "^13.2.2",
    "pnpm": "^8.6.0",
    "react": "^18.2.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.27.2",
    "rollup-plugin-dts": "^5.3.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vitest": "^0.31.3",
    "vue": "^3.3.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
